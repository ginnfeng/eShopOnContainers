{
  "ReRoutes": [    
    {
      "DownstreamPathTemplate": "/api/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "service.banking.api",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/api/b/{everything}",
      "UpstreamHttpMethod": [ "POST", "PUT", "GET" ],
      "SwaggerKey": "banking"
    },
    {
      "DownstreamPathTemplate": "/api/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "service.ordering.api",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/api/o/{everything}",
      "UpstreamHttpMethod": [ "POST", "PUT", "GET" ],
      "SwaggerKey": "ordering"
    }
  ],
  "SwaggerEndPoints": [    
    {
      "Key": "ordering",
      "Config": [
        {
          "Name": "Odering API",
          "Version": "v1",
          "Url": "http://service.ordering.api/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "banking",
      "Config": [
        {
          "Name": "Banking API",
          "Version": "v1",
          "Url": "http://service.banking.api/swagger/v1/swagger.json"
        }
      ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost"
  }
}
